{
  "name": "levelup-watch",
  "version": "1.1.0",
  "description": "Scaffolding for a watch app",
  "author": "LevelUp",
  "repository": {
    "type": "git",
    "url": "https://github.com/twlevelup/watch_edition"
  },
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --inline --hot",
    "build": "webpack",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "ci": "npm -s run stylecheck && npm -s run test",
    "deploy": "gh-pages -d public -u 'levelup <levelup@thoughtworks.com>' -m 'Deploy to Github Pages' -t",
    "todos": "fixme -p client/src/js && fixme -p client/spec/",
    "stylecheck": "eslint client/src/js/ client/spec/ --fix",
    "postinstall": "cd ./framework && npm install"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-class-properties": "6.13.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.13.0",
    "file-loader": "^2.0.0",
    "handlebars-template-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.10",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "watch-framework": "file:framework"
  },
  "engines": {
    "node": "10.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "copy-webpack-plugin": "^4.5.2",
    "fixme": "^0.5.0",
    "gh-pages": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.4.2",
    "jest-handlebars": "^1.0.1",
    "jsdom": "^13.0.0",
    "regenerator-runtime": "^0.13.1",
    "request": "^2.87.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  }
}
